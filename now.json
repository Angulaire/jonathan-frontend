{
  "build": {
    "env": {
      "API_BASE_URL": "@api_base_url",
      "CLOUDINARY_CLOUD_NAME": "@cloudinary_cloud_name"
    }
  },
  "routes": [
    {
      "src": "/page-sitemap.xml",
      "status": 301,
      "headers": { "Location": "/sitemap.xml" }
    },
    {
      "src": "/portfolio-sitemap.xml",
      "status": 301,
      "headers": { "Location": "/sitemap.xml" }
    },
    {
      "src": "/post-sitemap.xml",
      "status": 301,
      "headers": { "Location": "/sitemap.xml" }
    },
    {
      "src": "/sitemap_index.xml",
      "status": 301,
      "headers": { "Location": "/sitemap.xml" }
    },
    {
      "src": "/certification-digital-active-de-google",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/transformer-un-visiteur-en-lecteur/",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/author/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/tag/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/wp-content/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/blog/page/(.*)",
      "status": 301,
      "headers": { "Location": "/" }
    }
  ]
}